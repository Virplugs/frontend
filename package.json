{
  "name": "@virplugs/virplugs",
  "version": "1.0.0",
  "description": "Virplugs DAW",
  "repository": {
    "type": "git",
    "url": "https://github.com/Virplugs/Virplugs.git"
  },
  "main": "dist/electron.js",
  "scripts": {
    "check-licenses": "virplugs-check-licenses",
    "postinstall": "electron-builder install-app-deps && npm-run-all check-licenses",
    "start:electron": "electron .",
    "start": "npm run build:electron && concurrently --success first --kill-others \"vue-cli-service serve --mode=development\" \"wait-on http://localhost:8123/ && electron . --enable-logging --enable-api-filtering-logging\"",
    "serve": "vue-cli-service serve",
    "build:app": "vue-cli-service build",
    "build:ci": "vue-cli-service build --report",
    "build:electron": "webpack --config electron.webpack.config.js",
    "start:prod": "cross-env-shell NODE_ENV=production \"npm run build:app && npm run build:electron && electron .\"",
    "lint:eslint": "vue-cli-service lint",
    "lint:stylelint": "stylelint \"src/**/*.{vue,css,less,html}\"",
    "lint": "npm-run-all lint:*",
    "pack": "cross-env-shell NODE_ENV=production \"npm-run-all clean && npm-run-all build:app && npm-run-all build:electron && electron-builder --dir\"",
    "clean": "rimraf dist && rimraf pack",
    "dist:win": "electron-builder --win portable"
  },
  "build": {
    "appId": "com.virplugs",
    "productName": "Virplugs",
    "copyright": "Copyright Â© 2020 Virplugs",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "buildResources": "resources",
      "output": "./pack"
    },
    "files": [
      "dist/**"
    ]
  },
  "author": "Virplugs",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@virplugs/audioengine": "github:virplugs/audioengine",
    "fswin": "^3.19.908"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@virplugs/license-checker": "github:virplugs/license-checker",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-typescript": "~4.5.4",
    "@vue/cli-service": "^4.5.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "electron": "^11.0.1",
    "electron-builder": "^22.8.0",
    "electron-is-dev": "^1.2.0",
    "electron-modal": "^1.0.0",
    "electron-rebuild": "^2.0.1",
    "electron-splashscreen": "^1.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^7.0.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.19",
    "lodash.debounce": "^4.0.8",
    "mousetrap": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "raw-loader": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rete": "^1.4.4",
    "rete-area-plugin": "^0.2.1",
    "rete-auto-arrange-plugin": "^0.4.0",
    "rete-comment-plugin": "^0.7.0-rc.1",
    "rete-connection-mastery-plugin": "^0.2.4",
    "rete-connection-plugin": "^0.9.0",
    "rete-context-menu-plugin": "^0.6.0-rc.1",
    "rete-history-plugin": "^0.2.1",
    "rete-keyboard-plugin": "^0.1.2",
    "rete-minimap-plugin": "^0.3.1",
    "rete-task-plugin": "^0.3.0",
    "rete-vue-render-plugin": "^0.5.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "typescript": "~4.0.2",
    "ubuntu-fontface": "^0.1.13",
    "uuid": "^8.3.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-devtools": "^5.1.4",
    "vue-observe-visibility": "^0.4.6",
    "vue-property-decorator": "^9.0.0",
    "vue-resize-split-pane": "^0.1.5",
    "vue-template-compiler": "^2.6.11",
    "vuedraggable": "^2.24.0",
    "wait-on": "^5.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
